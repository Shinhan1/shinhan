<script type="text/javascript">
	$(function(){
		$('#btn1').click(function(){
			// right 버튼을 클릭하면 img가 오른쪽으로 800 이동시켜보세요
			$('img').css("margin-left", "800px");
			
			
			
		});		// btn1 click end
	});
</script>
